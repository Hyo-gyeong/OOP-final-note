//난수생성
import java.util.Random;
public class PracticeRe52 {

	public static void main(String[] args) {
		Random rn = new Random();
		
		for (int i = 1; i <= 10000; i++) {
			int randNum = rn.nextInt(9000)+1000; // 정수 랜덤으로 변수에 넣음
			System.out.print(randNum+"\t");
			if (i % 10 == 0) {
				System.out.println();
			}
		}
		
	}

}
