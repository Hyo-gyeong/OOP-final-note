
public class Shape {
	private int x, y;
	
	public void setX(int x) {
		this.x = x;
	}
	public int setY() {
		return this.y;
	}
	
	public int calcArea() {
		return x*y;
	}
	
	public String toString() {
		return "타입 : 도형"+positionStr();
	}
	public String positionStr() {
		return "("+x+", "+y+")";
	}
}
