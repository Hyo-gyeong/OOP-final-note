public class Date  {
	private int year; 
	private int month; 
	private int day;
	
	public void setYear(int year) {
		
	}
	public void setMonth(int month) {
	
	}
	public void setDay(int day) {
		
	}
	
	public String toString() {
		return year+"년 "+month+"월 "+day+"일 입니다.\n";
	} // 설정자 메소드 수정 // 접근자 메소드
}