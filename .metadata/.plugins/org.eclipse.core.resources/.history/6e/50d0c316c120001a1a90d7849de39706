
public class Box {
	private int x, y, z;
	private boolean empty;
	
	public void setX(int x) {
		this.x = x;
	}
	public void setY(int y) {
		this.y = y;
	}
	public void setZ(int z) {
		this.z = z;
	}
	
	public int getX() {
		return this.x;
	}
	public int getY() {
		return this.y;
	}
	public int getZ() {
		return this.z;
	}
	
	public void fillBox() {
		this.empty = false;
	}
	public void emptyBox() {
		this.empty = true;
	}
}
