
public class PracticeRe60 {

	public static void main(String[] args) {
		Animal a = new Animal();
		a.picture = "동물 그림"; 
		a.weight = 100; // compile error!!
		a.printMsg(); // compile error!! 
		a.eat(); a.sleep();
		a.roar(); // compile error!!
		
		Lion lion = new lion();
		lion.picture = "사자 그림";
		lion.eat(); lion.sleep();
		lion.roar(); lion.fly(); // compile error!!
		
		Eagle eagle = new Eagle();
		eagle.eat();
		eagle.sleep(); 
		eagle.fly();

	}

}
